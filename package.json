{
  "name": "yimby",
  "version": "0.0.0",
  "description": "hack the planet / legalize housing",
  "main": "index.js",
  "author": "DC Posch",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "deck.gl": "^3.1.1",
    "immutable": "^3.8.1",
    "luma.gl": "^2.10.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-map-gl": "^1.7.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "http-server": "^0.9.0",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.8.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run test && npm run watch & http-server static/",
    "build": "rm -rf static/build && mkdir static/build && ./scripts/process-data.js && npm run browserify",
    "watch": "watchify -t babelify src/app.js -o static/build/bundle.js",
    "browserify": "browserify -t babelify src/app.js | uglifyjs -c -m > static/build/bundle.js",
    "test": "standard"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
