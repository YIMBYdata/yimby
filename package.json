  "dependencies": {
    "deck.gl": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "immutable": "^3.8.2",
    "luma.gl": "^5.2.0",
    "node-static": "^0.7.9",
    "parcel-bundler": "^1.7.1",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-map-gl": "^3.2.7",
    "regenerator-runtime": "^0.11.1",
    "standard": "^11.0.1"
  },
  "scripts": {
    "start": "npm run test && npm run watch & npm run serve",
    "serve": "cd static && static -z --cache false .",
    "build": "rm -rf static/build && mkdir static/build && ./scripts/process-data.js && npm run build-js",
    "copy-js": "cp node_modules/mapbox-gl/dist/mapbox-gl.css static/build/",
    "build-js": "npm run copy-js && parcel build src/app.js -d static/build",
    "watch": "npm run copy-js && parcel watch src/app.js -d static/build",
    "test": "standard"
  }
}
